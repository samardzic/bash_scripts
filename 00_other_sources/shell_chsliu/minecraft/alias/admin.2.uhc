uhctitle() {
    num=$(echo -e $(date +%m)+5 | bc)
    dates=$(date '+%Y/%m/%d')
    weekBegin=$(date --date "$dates -`date +%d` days +1 days" +%U)
    lastSatofMon=$(ncal -h |awk '/Sa/ {print $(NF)}')
    weekEnd=$(date --date "$dates -`date +%d` days +$lastSatofMon days" +%U)
    weekNow=$(date +%U)
    weekDiff=$(echo -e $weekNow - $weekBegin + 1 | bc)
    # headline=$([ $weekNow == $weekEnd ] && echo -e ===== || echo -e ====)
    title=$([ $weekNow == $weekEnd ] && echo -e æ±ºè³½ || echo -e æ­¡æ¨‚å ´$(num2roman $weekDiff))
    echo -e ç¬¬$numå±†uhcè‹¦åŠ›æ€•ç›ƒ$title
}


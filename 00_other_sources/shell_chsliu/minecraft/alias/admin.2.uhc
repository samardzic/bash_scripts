log_parse_seed() {
    [ ! -f "$1" ] && return
    logfile=$(pwd)/teams.log
    tmp=/tmp/teams.log
    rm $tmp
    cat $1 | grep 'Seed:' | awk '{ printf ("%s\n", $5) }' >>$tmp
    cat $1 | grep 'Seed:' | awk '{ printf ("%s\n", $11) }' >>$tmp
    file.replace.escape $tmp
    cat $tmp | grep -v start | sed "s/)//g" | sed '/^\s*$/d' | sort | uniq >$logfile
    
    cat $logfile
}

